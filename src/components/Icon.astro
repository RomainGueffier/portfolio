---
import { Icon as AstroIcon } from 'astro-icon/components'
export interface Props {
  name: string
  width?: string | number
  height?: string | number
  id?: string
  style?: object
  className?: string
}

const { name, className, ...attributes } = Astro.props as Props
---

<AstroIcon name={`tabler:${name}`} class={className} {...attributes} />
